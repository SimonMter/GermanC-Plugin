{
    "Integer Declaration": {
        "prefix": "ganzzahl",
        "body": ["ganzzahl ${1:variableName} = ${2:value};"],
        "description": "Declare an integer variable"
    },
    "Character Declaration": {
        "prefix": "bitzeichen",
        "body": ["bitzeichen ${1:variableName} = '${2:char}';"],
        "description": "Declare a character variable"
    },
    "Return Statement": {
        "prefix": "zurück",
        "body": ["zurück ${1:value};"],
        "description": "Return a value"
    },
    "If Statement": {
        "prefix": "soferne",
        "body": [
            "soferne (${1:condition}) {",
            "    ${2:// code}",
            "}"
        ],
        "description": "If statement"
    },
    "Else Statement": {
        "prefix": "andernfalls",
        "body": [
            "andernfalls {",
            "    ${1:// code}",
            "}"
        ],
        "description": "Else statement"
    },
    "While Loop": {
        "prefix": "während",
        "body": [
            "während (${1:condition}) {",
            "    ${2:// code}",
            "}"
        ],
        "description": "While loop"
    },
    "For Loop": {
        "prefix": "durchlaufe",
        "body": [
            "durchlaufe (${1:ganzzahl i = 0}; ${2:i < n}; ${3:i++}) {",
            "    ${4:// code}",
            "}"
        ],
        "description": "For loop"
    },
    "Void Function": {
        "prefix": "nichtig",
        "body": [
            "nichtig ${1:functionName}(${2:parameters}) {",
            "    ${3:// code}",
            "}"
        ],
        "description": "Void function"
    },
    "Main Function": {
        "prefix": "ursprung",
        "body": [
            "ganzzahl ursprung() {",
            "    ${1:// code}",
            "    zurück 0;",
            "}"
        ],
        "description": "Main function"
    },
    "Break Statement": {
        "prefix": "unterbrech",
        "body": ["unterbrech;"],
        "description": "Break statement"
    },
    "Continue Statement": {
        "prefix": "weitergehen",
        "body": ["weitergehen;"],
        "description": "Continue statement"
    },
